#include <stdio.h>

int main() {
    int m, p, i, j;
    int mem[10], proc[10], alloc[10];

    printf("Enter number of memory blocks: ");
    scanf("%d", &m);
    printf("Enter sizes of memory blocks:\n");
    for (i = 0; i < m; i++)
        scanf("%d", &mem[i]);

    printf("Enter number of processes: ");
    scanf("%d", &p);
    printf("Enter sizes of processes:\n");
    for (i = 0; i < p; i++)
        scanf("%d", &proc[i]);

    for (i = 0; i < p; i++)
        alloc[i] = -1;

    for (i = 0; i < p; i++) {
        int worst = -1;
        for (j = 0; j < m; j++) {
            if (mem[j] >= proc[i]) {
                if (worst == -1 || mem[j] > mem[worst])
                    worst = j;
            }
        }

        if (worst != -1) {
            alloc[i] = worst;
            mem[worst] -= proc[i];
        }
    }

    printf("\nProcess No\tProcess Size\tBlock No\n");
    for (i = 0; i < p; i++) {
        printf("%d\t\t%d\t\t", i + 1, proc[i]);
        if (alloc[i] != -1)
            printf("%d\n", alloc[i] + 1);
        else
            printf("Not Allocated\n");
    }

    return 0;
}
