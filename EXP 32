#include <stdio.h>

int main()
{
    int pages[20], frames[10], time[10];
    int n, f, i, j, k, faults = 0, t = 0;

    printf("Enter number of pages: ");
    scanf("%d", &n);

    printf("Enter page reference string: ");
    for (i = 0; i < n; i++)
        scanf("%d", &pages[i]);

    printf("Enter number of frames: ");
    scanf("%d", &f);

    for (i = 0; i < f; i++)
        frames[i] = -1;

    for (i = 0; i < n; i++)
    {
        int hit = 0;
        t++;

        for (j = 0; j < f; j++)
        {
            if (frames[j] == pages[i])
            {
                hit = 1;
                time[j] = t;
                break;
            }
        }

        if (!hit)
        {
            int lru = 0;
            for (j = 1; j < f; j++)
                if (time[j] < time[lru])
                    lru = j;

            frames[lru] = pages[i];
            time[lru] = t;
            faults++;
        }

        for (j = 0; j < f; j++)
            printf("%d ", frames[j]);
        printf("\n");
    }

    printf("Total Page Faults = %d\n", faults);
    return 0;
}
